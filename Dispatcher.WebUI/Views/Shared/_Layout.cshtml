<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <meta name="description" content="Dispetçer Xidmət Proqramı">
    <meta name="author" content="Abbasov Ceyhun">
    <meta name="keyword" content="ADNSU, Dispetçer, Xidmət, Proqramı">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="icon" href="~/Content/Images/dispatcher.ico" />
    @Styles.Render("~/Content/Styles/GeneralStyles")
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

</head>
<body>
    <div class="navbar">
        <div class="navbar-inner">
            <div class="container-fluid">
                @if (ViewBag.BackUrlVisibility)
                {
                    <a class="brand" href="@ViewBag.BackUrl">
                        <i class="icon-chevron-left"></i>
                    </a>
                }
                <a class="@(ViewBag.BackUrlVisibility ? "non" : "") brand"><span>Dispetçer Xidmət Proqramı - @ViewBag.Title</span></a>
            </div>
        </div>
    </div>

    <div class="container-fluid-full">
        <div class="row-fluid">
            <div id="content" class="span10">
                @RenderBody()
            </div>
        </div>
    </div>

    
    @Scripts.Render("~/Content/Scripts/GeneralScripts")
    @*<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>*@
    @RenderSection("Scripts", false)
</body>
</html>